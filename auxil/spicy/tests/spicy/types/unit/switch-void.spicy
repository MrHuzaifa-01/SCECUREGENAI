module Mini;

public type X = unit {
    a: bytes &size=1;

    switch ( self.a ) {
        b"0"       -> : void; # No hook.
        b"1"       -> : void { print "1"; }
        b"2", b"3" -> : void { print "2 or 3"; }
        *          -> : void { print "something else"; }
    };
};
